# Common shell aliases
alias cdo='cd $OLDPWD'
alias r='reset'
alias a='ls -l'
alias aa='ls -la'
alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'

# Git aliases (Kept here so that all aliases are in one place)
alias s='git status'
alias glog='git log --all --graph --decorate=auto'

# Alias for screen. I like to keep as many conf files
# in $HOME/.dotfiles and not $HOME.
alias screen='screen -c ${HOME}/.dotfiles/screenrc'
